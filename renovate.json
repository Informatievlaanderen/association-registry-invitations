{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": ["^paket.dependencies$"],
      "matchStrings": ["nuget (?<depName>[^ ]+) (?<currentValue>[^ ]+)"],
      "datasourceTemplate": "nuget",
      "versioningTemplate": "semver"
    }
  ]
}
